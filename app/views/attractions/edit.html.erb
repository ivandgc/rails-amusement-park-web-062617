<%= form_for @attraction do |f| %>
  <h1><%= f.text_field :name, value: @attraction.name %></h1>
  <label><%= f.text_field :tickets, value: @attraction.tickets %></label>
  <label><%= f.text_field :nausea_rating, value: @attraction.nausea_rating %></label>
  <label><%= f.text_field :happiness_rating, value: @attraction.happiness_rating  %></label>
  <label><%= f.text_field :min_height, value: @attraction.min_height %></label>
  <%= f.submit %>
<% end %>
